# Uncomment the next line to define a global platform for your project
 platform :ios, '11.0'

target 'iOS-101-day5' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOS-101-day5
  pod 'Alamofire', '~> 5.1'
  pod 'Kingfisher', '~> 5.0'
  pod 'SnapKit', '~> 5.0.0'
  pod "ESPullToRefresh", '~> 2.9.2'

  target 'iOS-101-day5Tests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

end
